{
  "buildCommand": "cd kalshi-dashboard && npm install && npm run build",
  "outputDirectory": "kalshi-dashboard/dist",
  "devCommand": "cd kalshi-dashboard && npm run dev",
  "installCommand": "cd kalshi-dashboard && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/kalshi/:path*",
      "destination": "https://api.elections.kalshi.com/trade-api/v2/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), payment=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; connect-src 'self' https://api.the-odds-api.com https://api.elections.kalshi.com; img-src 'self' data:; font-src 'self' data:; object-src 'none'; base-uri 'self';"
        }
      ]
    }
  ]
}
